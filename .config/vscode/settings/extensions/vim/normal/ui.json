{
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "u",
        "w"
      ],
      "commands": [
        "editor.action.toggleWordWrap"
      ]
    },
    {
      "before": [
        "<leader>",
        "u",
        "c"
      ],
      "commands": [
        "cSpell.toggleEnableSpellChecker"
      ]
    },
  ]
}
